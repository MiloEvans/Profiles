# 绝大部分浏览器均可使用
# 浏览器搜索引擎更改为 DuckDuckGO 或 搜狗（建议优先 DuckDuckGO 没有的选 搜狗）
# 搜索框内输入命令+空格+关键词快速指定搜索引擎

# > 资料
# gh: GitHub 搜索
# gp: Google 图片搜索
# bk: 百度百科
# wk: 维基百科
# qd: 起点
# ys: 优书网
# jl: javlibrary
# db: JavDB

# > 翻译
# trc: Google 译至中
# tre: Google 译至英
# trj: Google 译至日

# > 社区
# db: 豆瓣搜索
# zh: 知乎
# tw: 推特

# > 购物
# jd: 京东
# tb: 淘宝

# > 视频资源
# yg: YouTube
# ph: PornHub 
# ag: avgle
# jb: Jable
# nv: Netflav
# jh: Jav HD
 
# > 默认搜索
# bi: 必应
# bd: 百度搜索
# ddg: DuckDuckGo 搜索
# 无指令则转 Google 搜索

[URL Rewrite]
# 搜狗 302 DuckDuckGo
^https?:\/\/((www|m)\.)?sogou\.com\/web(\/.+)?\?(.*&)?(query|keyword)=([^&]+).* https://duckduckgo.com/?q=$6&t=iphone 302

# > 资料
# gh GitHub
^https:\/\/duckduckgo.com\/\?q=gh(\+|%20)([^&]+).* https://github.com/search?q=$2 302

# gp Google图片
^https:\/\/duckduckgo.com\/\?q=gp(\+|%20)([^&]+).* https://www.google.com/search?&tbm=isch&q=$2 302

# bk 百度百科
^https:\/\/duckduckgo.com\/\?q=bk(\+|%20)([^&]+).* https://baike.baidu.com/search?word=$2 302

# wk 维基百科
^https:\/\/duckduckgo.com\/\?q=wk(\+|%20)([^&]+).* https://zh.m.wikipedia.org/wiki/$2 302

# qd 起点
^https:\/\/duckduckgo.com\/\?q=qd(\+|%20)([^&]+).* https://m.qidian.com/search?kw=$2 302

# ys 优书网
^https:\/\/duckduckgo.com\/\?q=ys(\+|%20)([^&]+).* https://www.yousuu.com/search/?search_type=title&search_value=$2&from=search 302

# jl javlibrary
^https:\/\/duckduckgo.com\/\?q=jl(\+|%20)([^&]+).* http://www.javlibrary.com/cn/vl_searchbyid.php?keyword=$2 302

# db JavDB
^https:\/\/duckduckgo.com\/\?q=db(\+|%20)([^&]+).* https://javdb.com/search?q=$2 302

# > 翻译
# trc Google 译至中
^https:\/\/duckduckgo.com\/\?q=trc(\+|%20)([^&]+).* https://translate.google.com/#view=home&op=translate&sl=auto&tl=zh-CN&text=$2 302

# tre Google 译至英
^https:\/\/duckduckgo.com\/\?q=tre(\+|%20)([^&]+).* https://translate.google.com/#view=home&op=translate&sl=auto&tl=en&text=$2 302

# trj Google 译至日
^https:\/\/duckduckgo.com\/\?q=trj(\+|%20)([^&]+).* https://translate.google.com/#view=home&op=translate&sl=auto&tl=ja&text=$2 302

# > 社区
# db 豆瓣
^https:\/\/duckduckgo.com\/\?q=db(\+|%20)([^&]+).* https://m.douban.com/search/?query=$2 302

# zh 知乎
^https:\/\/duckduckgo.com\/\?q=zh(\+|%20)([^&]+).* https://www.zhihu.com/search?type=content&q=$2 302

# tw Twitter
^https:\/\/duckduckgo.com\/\?q=tw(\+|%20)([^&]+).* https://twitter.com/search?q=$2 302

# > 购物
# jd 京东
^https:\/\/duckduckgo.com\/\?q=jd(\+|%20)([^&]+).* https://so.m.jd.com/ware/search.action?keyword=$2 302

# tb 淘宝
^https:\/\/duckduckgo.com\/\?q=tb(\+|%20)([^&]+).* https://s.m.taobao.com/h5?q=$2 302

# > 视频资源
# yg YouTube
^https:\/\/duckduckgo.com\/\?q=yg(\+|%20)([^&]+).* https://www.youtube.com/results?search_query=$2 302

# ph PornHub
^https:\/\/duckduckgo.com\/\?q=ph(\+|%20)([^&]+).* https://cn.pornhub.com/video/search?search=$2 302

# ag avgle
# ^https:\/\/duckduckgo.com\/\?q=ag(\+|%20)([^&]+).* https://avgle.com/search/videos?search_query=$2&search_type=videos 302

^https:\/\/duckduckgo.com\/\?q=ag(\+|%20)([^&]+).* https://avgle.io/search/$2/1 302

# jb Jable
^https:\/\/duckduckgo.com\/\?q=jb(\+|%20)([^&]+).* https://jable.tv/search/?q=$2 302

# nv Netflav
^https:\/\/duckduckgo.com\/\?q=nv(\+|%20)([^&]+).* https://netflav.com/search?type=title&keyword=$2 302

# jh Jav HD
^https:\/\/duckduckgo.com\/\?q=jh(\+|%20)([^&]+).* https://www2.javhdporn.net/search/$2 302

# > 默认搜索
# bi 必应
^https:\/\/duckduckgo.com\/\?q=bi(\+|%20)([^&]+).* https://cn.bing.com/search?q=$2 302

# bd 百度
^https:\/\/duckduckgo.com\/\?q=bd(\+|%20)([^&]+).* https://www.baidu.com/s?wd=$2 302

# ddg DuckDuckGo 
^https:\/\/duckduckgo.com\/\?q=ddg(\+|%20)([^&]+).* https://duckduckgo.com/?ia=about&q=$2 302

# 无指令 谷歌
^https:\/\/duckduckgo.com\/\?q=([^&]+).* https://www.google.com/search?q=$1 302

[MITM]
hostname = %APPEND% duckduckgo.com, *.sogou.com
