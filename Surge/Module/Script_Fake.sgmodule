#!name=𝐒𝐜𝐫𝐢𝐩𝐭 - 𝐅𝐚𝐤𝐞
#!desc=破解类脚本-𝑀𝑖𝑙𝑜𝐸𝑣𝑎𝑛𝑠
#!system=ios

[Script]
# > 𝐥𝐚𝐧𝐠𝐤𝐡𝐚𝐜𝐡

# 𝐋𝐢𝐠𝐡𝐭𝐫𝐨𝐨𝐦
# hostname = photos.adobe.io
𝐅𝐚𝐤𝐞-𝐋𝐢𝐠𝐡𝐭𝐫𝐨𝐨𝐦 = requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/langkhach270389/Scripting/master/Lightroom.js,script-update-interval=0,type=http-response,pattern=^https:\/\/photos\.adobe\.io\/v2\/accounts

# 𝐏𝐡𝐨𝐭𝐡𝐨𝐩&𝐏𝐒𝐞𝐱𝐩𝐫𝐞𝐬𝐬
𝐅𝐚𝐤𝐞-𝐏𝐡𝐨𝐭𝐨𝐬𝐡𝐨𝐩 = requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/langkhach270389/Scripting/master/photoshop.js,script-update-interval=0,type=http-response,pattern=^https:\/\/lcs-mobile-cops\.adobe\.io\/mobile_profile

# 𝐏𝐢𝐜𝐬𝐀𝐫𝐭美易
# hostname = api.picsart.c*, api.meiease.c*
𝐅𝐚𝐤𝐞-𝐏𝐢𝐜𝐬𝐀𝐫𝐭 = requires-body=1,script-path=https://raw.githubusercontent.com/langkhach270389/Scripting/master/picsArt.vip.js,type=http-response,pattern=^https:\/\/api\.(picsart|meiease)\.c(n|om)\/users\/show\/me\.json

# 𝐕𝐒𝐂𝐎滤镜𝐕𝐈𝐏
# hostname = vsco.co
𝐅𝐚𝐤𝐞-𝐕𝐒𝐂𝐎 = requires-body=1,script-path=https://raw.githubusercontent.com/langkhach270389/Scripting/master/vsco.js,type=http-response,pattern=^https?:\/\/vsco\.co\/api\/subscriptions\/2.1\/user-subscriptions\/

# 𝐒𝐩𝐥𝐢𝐜𝐞视频编辑器
# hostname = splice.oracle.*.com
𝐅𝐚𝐤𝐞-𝐒𝐩𝐥𝐢𝐜𝐞 = requires-body=1,script-path=https://raw.githubusercontent.com/langkhach270389/Scripting/master/splice.js,type=http-response,pattern=^https:\/\/splice\.oracle\.\w+\.com\/devices\/me

# 𝐂𝐚𝐦𝐒𝐜𝐚𝐧𝐧𝐞𝐫扫描全能王
# hostname = ap*.intsig.net
𝐅𝐚𝐤𝐞-𝐂𝐚𝐦𝐒𝐜𝐚𝐧𝐧𝐞𝐫 = requires-body=1,script-path=https://raw.githubusercontent.com/langkhach270389/Scripting/master/CamScaner.js,type=http-response,pattern=^https:\/\/(api|api-cs)\.intsig\.net\/purchase\/cs\/query_property\?


# > 𝐍𝐨𝐛𝐲𝐃𝐚

# 酷我音乐𝐒𝐕𝐈𝐏 (𝐁𝐲 𝐲𝐱𝐢𝐚𝐨𝐜𝐚𝐢)
# hostname = *.kuwo.cn
𝐅𝐚𝐤𝐞-酷我音乐 = requires-body=1,script-path=https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/KuWoMusicDownload.js,type=http-request,pattern=^https?:\/\/musicpay\.kuwo.cn\/music\.pay\?uid=\d+
𝐅𝐚𝐤𝐞-酷我音乐 = requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/Kuwo.js,type=http-response,pattern=^https?:\/\/vip1\.kuwo\.cn\/(vip\/v2\/user\/vip|vip\/spi/mservice)

# 𝐕𝐔𝐄 𝐩𝐫𝐨
# hostname = api.vuevideo.net
𝐅𝐚𝐤𝐞-𝐕𝐔𝐄 = requires-body=1,script-path=https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/VUE.js,type=http-response,pattern=^https:\/\/api\.vuevideo\.net\/api\/v1\/(users\/.+\/profile|subtitle\/prepare)


# > 𝐧𝐳𝐰𝟗𝟑𝟏𝟒

# 轻颜相机 & 𝐮𝐥𝐢𝐤𝐞 & 蒸汽波相机(𝐯𝐚𝐩𝐨𝐫𝐜𝐚𝐦)三合一 解锁𝐕𝐈𝐏(𝐁𝐲 @𝐬 𝐲 & 𝐀𝐥𝐞𝐱𝟎𝟓𝟏𝟎)
# hostname = commerce-i18n-api.faceu.mobi ,commerce-api.faceu.mobi, pay.wecut.com
𝐅𝐚𝐤𝐞-轻颜相机 = requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/nzw9314/QuantumultX/master/Script/qyxj.js,type=http-response,pattern=^https:\/\/commerce-.*api\.faceu\.mobi\/commerce\/v1\/subscription\/user_info

# 有道云笔记𝐕𝐈𝐏 (𝐁𝐲𝐀𝐥𝐞𝐱𝟎𝟓𝟏𝟎)
# hostname = note.youdao.com
𝐅𝐚𝐤𝐞-有道云笔记 = requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/Alex0510/Eric/master/surge/Script/ydybj.js,type=http-response,pattern=https://note.youdao.com/yws/(mapi/payment|api/self)

# 马卡龙 (𝐁𝐲 @𝐂𝐡𝐞𝐞𝐫𝐲𝐓𝐨𝐝𝐨)
# hostname = app.api.versa-ai.com
𝐅𝐚𝐤𝐞-马卡龙 = requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/nzw9314/QuantumultX/master/Script/mkl.js,type=http-response,pattern=https://app.api.versa-ai.com/pay/order/iap/check

# 美颜相机一次性解锁内购（𝐁𝐲黑黑酱）
# hostname = api.meiyan.com
𝐅𝐚𝐤𝐞-美颜相机 = requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/nzw9314/QuantumultX/master/Script/myxj.js,type=http-response,pattern=^https:\/\/api\.meiyan\.com\/iap\/verify\.json

